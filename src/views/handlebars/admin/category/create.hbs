{{#*inline "form"}}
<form enctype="application/x-www-form-urlencoded" action="/admin/categories/create" method="post" name="category">
    <div class="mb-3">
        <label for="name" class="form-label">Name</label>
        <input type="text" class="form-control" name="attributes[name]">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
{{/inline}}
{{>create modelName='category'}}
