{{#*inline "form"}}
<form enctype="application/x-www-form-urlencoded" action="/admin/stores/update" method="post" name="store">
    <div class="mb-3">
        <label for="name" class="form-label">Name</label>
        <input type="text" class="form-control" name="attributes[name]" value="{{store.attributes.name}}">
    </div>
    <button type="submit" class="btn btn-primary">Submit</button>
</form>
{{/inline}}
{{>update modelName='store'}}
